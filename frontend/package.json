		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.20.2",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.1",
		"vite": "^5.4.14",
		"vite-plugin-kit-routes": "^0.6.11",
		"vite-plugin-mkcert": "^1.17.6",
		"vitest": "^2.1.9"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.7.0",
		"@oslojs/jwt": "^0.3.0",
		"@pilcrowjs/object-parser": "^0.0.4",
		"arctic": "^1.9.2",
		"bits-ui": "^0.21.16",
		"clsx": "^2.1.1",
